// 1
datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

// 2
generator client {
  provider = "prisma-client-js"
}

// 3

model Family {
  //id Int @id @default(autoincrement())
  id String  @id @default(uuid())
  name String
  numberOfPeople Int
}

model Member {
  //id Int @id @default(autoincrement())
  id String  @id @default(uuid())
  name String
  age Int
}